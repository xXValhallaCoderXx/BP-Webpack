{
    "presets": [
        ["es2015", { "modules": false}],
        "react",
        [
            "env",
            {
                "modules": false,
                "targets": {
                    "browsers": [
                        "ie >= 9",
                        "last 3 chrome versions",
                        "last 3 safari versions",
                        "last 3 firefox versions"
                    ]
                }
            }
        ]
    ],
    "env": {
        "development": {
            "plugins": ["react-hot-loader/babel"]
        },
        "production": {
            "plugins": ["transform-remove-console"]
        }
    },
    "plugins": ["syntax-dynamic-import"]
}